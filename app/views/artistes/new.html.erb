<h1> new</h1>
<div class="container">
  <div class="row">
    <div class="col-6 offset-3">
      <%= simple_form_for @artiste do |f| %>
        <%= f.input :name %>
        <%= f.input :genre %>
        <%= f.input :biography %>
        <%= f.input :description %>
        <%= f.input :les_dessous %>
        <%= f.input :youtube_link %>
        <%= f.input :youtube_nouveaute1 %>
        <%= f.input :youtube_nouveaute2 %>
        <%= f.input :youtube_nouveaute3 %>
        <%= f.input :youtube_nouveaute4 %>
        <%= f.input :instagram_link%>
        <%= f.input :spotify_link%>
        <%= f.input :photo, as: :file %>
        <%= f.input :reseau1, as: :file %>
        <%= f.input :reseau2, as: :file %>
        <%= f.submit "Create" %>
      <% end %>
    </div>
  </div>
</div>