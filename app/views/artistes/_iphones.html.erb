        <div class="iphonetest">
          <% if @artiste.reseau1.attached? %> 
            <div class="iphone1">
              <div class="title_logo">
                <p class="ecriture_insta">INSTAGRAM</p>
                <%= link_to @artiste.spotify_link, class:"redirection_reseaux_insta" do%>
                  <i class="fab fa-instagram fa-2x" id="instagram" ></i>
                <% end %>
              </div>
              <div class="iphone_structure">
                    <%= cl_image_tag @artiste.reseau1.key, crop: :fill, :class => 'img-s-reseau1' %>
              </div>
            </div>
          <% end %>
          <% if @artiste.reseau2.attached? %> 
            <div class="iphone1">
              <div class="title_logo">
                <p class="ecriture_spotify">SPOTIFY</p>
                <%= link_to @artiste.spotify_link, class:"redirection_reseaux_spotify" do%>
                  <i class="fab fa-spotify fa-2x" id="spotify"></i>
                <% end %>
              </div>
              <div class="iphone_structure">
                <%= cl_image_tag @artiste.reseau2.key, crop: :fill, :class => 'img-s-reseau2' %>
              </div>
            </div>
          <% end %>
          <% if @artiste.reseau3.attached? %> 
            <div class="iphone1">
              <div class="title_logo">
                <p class="ecriture_youtube">YOUTUBE</p>
                <%= link_to @artiste.youtube_link_for_contacts, class:"redirection_reseaux_youtube" do%>
                  <i class="fab fa-youtube fa-2x" id="youtube" ></i>
                <% end %>
              </div>
              <div class="iphone_structure">
                <%= cl_image_tag @artiste.reseau3.key, crop: :fill, :class => 'img-s-reseau3' %>
              </div>
            </div>
          <% end %>
          <% if @artiste.reseau4.attached? %> 
            <div class="iphone1">
              <div class="title_logo">
                <p class="ecriture_tiktok">TIKTOK</p>
                <%= link_to @artiste.tiktok_link, class:"redirection_reseaux_tiktok" do%>
                  <i class="fab fa-tiktok fa-2x", id="tiktok" ></i>
                <% end %>
              </div>
              <div class="iphone_structure">
                <%= cl_image_tag @artiste.reseau4.key, crop: :fill, :class => 'img-s-reseau4' %>
              </div>
            </div>
          <% end %>
        </div>